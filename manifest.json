{
    "manifest_version": 3,
    "name": "Tribun Hider",
    "version": "1.0.0",
    "description": "Hides the all videos from tribunnews.com",
    "icons": {
        "16": "icons/16.png",
        "32": "icons/32.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    },
    "content_scripts": [
        {
            "matches": ["https://www.youtube.com/*", "https://*.tribunnews.com/*"],
            "js": ["content.js"]
        }
    ],
    "permissions": [
        "activeTab",
        "tabs"
    ]
}